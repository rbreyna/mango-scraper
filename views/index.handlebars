
<div class="jumbotron">
  <h1><strong>Scraped Articles</strong></h1>
</div>

  {{#each Articles}}
  <div class="card">
    <div class="card-header">
      <a id={{@index}} href="https://www.nytimes.com{{link}}">  {{title}} </a>
    </div>
    <ul class="list-group list-group-flush">
      <li class="list-group-item">
        <button data-position={{@index}} class="btn btn-primary saveArticle">Save Article!</button>
      </li>
    </ul>
  </div><br><br>
  {{/each}}